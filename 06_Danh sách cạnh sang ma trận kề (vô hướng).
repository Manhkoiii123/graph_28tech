#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define pii pair<int,int>
const int MOD = (int)(1e9+7);
int n,m;
int a[1005][1005];
int main(){
    cin>>n>>m;
    for(int i=0;i<m;i++){
        int x,y;
        cin>>x>>y;
        a[x][y]=1;
        a[y][x]=1;
    }
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
    system("pause");
}
